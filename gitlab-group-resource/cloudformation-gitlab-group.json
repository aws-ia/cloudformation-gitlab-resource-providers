{
    "typeName": "Cloudformation::GitLab::Group",
    "description": "An example resource schema demonstrating some basic constructs and validation rules.",
    "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-rpdk.git",
    "properties": {
        "UID": {
            "description": "Unique identifier of the group",
            "type": "string"
        },
        "HostURL": {
            "description": "URL of the GitLab host server, e.g. https://gitlab.com",
            "type": "string"
        },
        "AccessToken": {
            "description": "Personal access token used to access the host server",
            "type": "string",
            "minLength": 20,
            "maxLength": 250
        },
        "GroupName": {
            "description": "Name of the group",
            "type": "string"
        },
        "Path": {
            "description": "Path of the group",
            "type": "string"
        },
        "ParentId": {
            "description": "ID of the group's parent",
            "type": "integer"
        }
    },
    "additionalProperties": false,
    "required": [
        "AccessToken",
        "HostURL"
    ],
    "readOnlyProperties": [
        "/properties/UID"
    ],
    "primaryIdentifier": [
        "/properties/UID"
    ],
    "handlers": {
        "create": {
            "permissions": []
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": []
        },
        "delete": {
            "permissions": []
        },
        "list": {
            "permissions": []
        }
    }
}
